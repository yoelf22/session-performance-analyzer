{
  "mcpServers": {
    "github-integration": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {}
    },
    "filesystem-access": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/path/to/allowed/files"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "deepgraph-react": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-code-graph@latest",
        "facebook/react"
      ],
      "env": {}
    },
    "deepgraph-typescript": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-code-graph@latest",
        "microsoft/TypeScript"
      ],
      "env": {}
    },
    "web-fetch": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-fetch"
      ],
      "env": {}
    }
  }
}